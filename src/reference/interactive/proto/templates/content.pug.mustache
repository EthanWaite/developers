{{#files}}
{{#file_messages}}
h2(id='interactive__type__{{message_name}}') {{message_name}}
p.
    {{#nobr}}{{message_description}}{{/nobr}}
table.table
    thead
        tr
            th Field
            th Type
            th Label
            th Description
    tbody
    {{#message_fields}}
        tr
            td.col-md-2 {{field_name}}
            td.col-md-2
                a(href='#interactive__type__{{field_type}}') {{field_type}}
            td.col-md-1 {{field_label}}
            td.col-md-7 {{#nobr}}{{field_description}}{{/nobr}}
{{/message_fields}}
{{/file_messages}}
{{/files}}

h2(id='interactive__scalar') Scalar Types
table.table
    thead
        tr
            th Type
            th C++ Type
            th Java Type
            th Python Type
            th Notes
    tbody
    {{#scalar_value_types}}
    tr
        td #[a(name='interactive__type__{{scalar_value_proto_type}}') {{scalar_value_proto_type}}]
        td {{scalar_value_cpp_type}}
        td {{scalar_value_java_type}}
        td {{scalar_value_python_type}}
        td {{#nobr}}{{scalar_value_notes}}{{/nobr}}
    {{/scalar_value_types}}
